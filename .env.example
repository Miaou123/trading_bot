# Trading Configuration
TRADING_MODE=paper  # paper or live
TRADING_ENABLED=true

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_API_KEY

# Wallet Configuration (KEEP SECURE!)
PRIVATE_KEY=your_base58_private_key_here
# Or use PRIVATE_KEY_FILE=./wallet.json

# Trading Strategy
INITIAL_INVESTMENT_SOL=0.1
MAX_CONCURRENT_POSITIONS=5
STOP_LOSS_PERCENTAGE=50
TAKE_PROFIT_1_PERCENTAGE=100  # 2x
TAKE_PROFIT_1_SELL_PERCENTAGE=50  # Sell 50% at 2x
TAKE_PROFIT_2_PERCENTAGE=300  # 4x
TAKE_PROFIT_2_SELL_PERCENTAGE=25  # Sell 25% at 4x
SLIPPAGE_TOLERANCE=5  # 5%

# Alert Listening Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
SCANNER_CHANNEL_ID=@your_scanner_channel
TRADING_RESULTS_CHANNEL_ID=@your_trading_results_channel

# Risk Management
MIN_TWITTER_LIKES=100
MIN_TWITTER_VIEWS=50000
BLACKLIST_BUNDLE_DETECTED=true
BLACKLIST_HIGH_RISK=true
MAX_DAILY_LOSSES_SOL=1.0

# Monitoring
POSITION_CHECK_INTERVAL=30000  # 30 seconds
PRICE_UPDATE_INTERVAL=10000    # 10 seconds
LOG_LEVEL=info

# Development
NODE_ENV=development
